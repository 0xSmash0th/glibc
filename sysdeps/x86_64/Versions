libc {
  GLIBC_2.14 {
    memcpy;
  }
}
libm {
  GLIBC_2.1 {
    # A generic bug got this omitted from other configurations' version
    # sets, but we always had it.
    exp2l;
  }
}
ld {
  GLIBC_2.26 {
    # The alternative x86-64 runtime interface to TLS with aligned stack.
    ___tls_get_addr;
  }
}
